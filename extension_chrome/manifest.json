{
    "manifest_version": 2,
    "name": "CHAEA",
    "version": "1",
    "browser_action": {
      "default_title": "CHAEA",
      "default_popup": "index.html",
      "default_icon": {
        "128": "128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery.min.js","content.js"]
      }
    ],
    "icons": {
      "128": "128.png"
    },
    "permissions": ["storage","activeTab","declarativeContent"]
}
